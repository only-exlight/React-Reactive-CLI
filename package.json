{
  "name": "react-reactive-cli",
  "version": "0.0.1",
  "description": "CLI for React-Reactive framework",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"tsc -w\" \"nodemon build/index.js\"",
    "start-new": "concurrently --kill-others \"tsc -w\" \"nodemon build/index.js -new MyProject\"",
    "start-create-c": "concurrently --kill-others \"tsc -w\" \"nodemon build/index.js -g c component\"",
    "start-create-s": "concurrently --kill-others \"tsc -w\" \"nodemon build/index.js -g s service\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snowstroom/React-Reactive-CLI.git"
  },
  "keywords": [
    "React",
    "CLI",
    "RxJS",
    "React-Reactive"
  ],
  "author": "eXlight",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/snowstroom/React-Reactive-CLI/issues"
  },
  "homepage": "https://github.com/snowstroom/React-Reactive-CLI#readme",
  "devDependencies": {
    "@types/node": "^10.12.1",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.5",
    "tslint": "^5.11.0",
    "typescript": "^3.1.4"
  },
  "dependencies": {
    "chalk": "^2.4.1"
  }
}
